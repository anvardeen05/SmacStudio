<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Smacar Studio" verbose="5">
	<groups>
		<run>
			<exclude name="common"></exclude>
			<exclude name="smoke"></exclude>
			<exclude name="regression"></exclude>
		</run>
	</groups> 
	<test name="Smacar Studio">
		<classes>
			<class name="testcases.TC001_Login" />
			<class name="testcases.TC002_LoginFailure" />
			<class name="testcases.TC003_CreateCampaign" />			
			<class name="testcases.TC004_UploadMarker" />
			<class name="testcases.TC005_PublishMarker" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
